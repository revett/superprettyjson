{
  "author": "Charlie Revett <superprettyjson@revdex.fastmail.com> (https://github.com/revett)",
  "name": "superprettyjson",
  "description": "Format JSON data in a colored YAML-style, perfect for CLI output",
  "version": "1.3.0",
  "homepage": "http://github.com/revett/superprettyjson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/revett/superprettyjson.git"
  },
  "bugs": {
    "url": "https://github.com/revett/superprettyjson/issues"
  },
  "keywords": [
    "syntax-highlighting",
    "cli",
    "json",
    "formatting",
    "colors",
    "pretty-print"
  ],
  "contributors": [
    {
      "name": "Rafael de Oleza",
      "email": "rafeca@gmail.com",
      "url": "https://github.com/rafeca"
    }
  ],
  "engines": {
    "node": ">=16"
  },
  "type": "commonjs",
  "main": "./lib/superprettyjson",
  "files": ["bin/superprettyjson", "lib/*.js"],
  "scripts": {
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "test": "npm run jshint && mocha --reporter spec",
    "testwin": "node ./node_modules/mocha/bin/mocha --reporter spec",
    "jshint": "jshint lib/*.js test/*.js",
    "coverage": "istanbul cover _mocha --report lcovonly -- -R spec",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "changelog": "git log $(git describe --tags --abbrev=0)..HEAD --pretty='* %s' --first-parent"
  },
  "bin": {
    "superprettyjson": "./bin/superprettyjson",
    "prettyjson": "./bin/superprettyjson",
    "spj": "./bin/superprettyjson"
  },
  "exports": {
    ".": {
      "require": "./lib/superprettyjson.js"
    }
  },
  "dependencies": {
    "@colors/colors": "1.5.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "should": "^11.1.1"
  }
}
